{
  "name": "DestructoChat",
  "version": "1.0.0",
  "description": "Self-Destructing Message App (MERN)",
  "main": "index.js",
  "engines": {
    "node": "8.9.2"
  },
  "scripts": {
    "build": "webpack -p --display-error-details --colors",
    "start": "NODE_ENV=production node server/index.js",
    "node:dev": "npm run node:watch & npm run node:webpack:watch",
    "node:watch": "nodemon server/index.js",
    "node:webpack:watch": "webpack -wd",
    "webpack:dev": "NODE_ENV=development webpack-dev-server",
    "postinstall": "npm run build",
    "seed": "node db/seed.js",
    "seed:watch": "nodemon db/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kang/DestructoChat.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d-kang/self-destruct/issues"
  },
  "homepage": "https://github.com/d-kang/self-destruct#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.2",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "moment": "^2.20.1",
    "mongoose": "^5.0.6",
    "mongoose-auto-increment": "^5.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
